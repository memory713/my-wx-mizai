<!--index.wxml-->
 <view class="container">
  <view class="userinfo">
    <button wx:if="{{!hasUserInfo && canIUse}}" open-type="getUserInfo" bindgetuserinfo="getUserInfo"> 获取头像昵称 </button>
    <block wx:else>
      <image bindtap="bindViewTap" class="userinfo-avatar" src="{{userInfo.avatarUrl}}" background-size="cover"></image>
      
    </block>
  </view>
  <scroll-view scroll-y="true" style="width: 100%; height: 100%">
  <view bindtouchend="touchEnd" bindtouchstart="touchStart" class="container">
    <view class="one {{ view[0].in }} {{ view[0].out }}" style="width: 100%; height: 100%">
      <image class="one-one {{ one_one }}" src="../../images/111.png"></image> 
       <!-- <image class="one-two {{ one_two }}" src="../../images/2.png"></image>  -->
       <view class="one-three {{ one_three }}">{{userInfo.nickName}}</view>
      <view class="one-four {{ one_four }}" style='margin-top:70rpx;'>welcome</view> 
       <!-- <view class="one-five {{ one_five }}">LW</view>  -->
     </view>
    <view class="two {{ view[1].in }} {{ view[1].out }}" style="width: 100%; height: 100%">
      <image class="two-one {{ two_one }}" src="../../images/7.png"></image>
      <image class="two-two {{ two_two }}" src="../../images/14.png"></image>
      <image class="two-three {{ two_three }}" src="../../images/3.png"></image>
      <image class="two-four {{ two_four }}" src="../../images/20.png"></image>
    </view>
    <image class="bottom-top {{ bottom_top }}" src="../../images/5.png"></image>
    <image class="bottom-one {{ bottom_one }}" src="../../images/1.png"></image>
    <image class="bottom-one2 {{ bottom_one }}" src="../../images/1.png"></image>
    <image class="bottom-two {{ bottom_two }}" src="../../images/11.png"></image>
    <image class="bottom-two2 {{ bottom_two }}" src="../../images/11.png"></image>
    <image class="bottom-three {{ bottom_three }}" src="../../images/12.png"></image>
    <image class="bottom-three2 {{ bottom_three }}" src="../../images/12.png"></image>
    <image class="bottom-four {{ bottom_four }}" src="../../images/10.png"></image>
    <image class="bottom-four2 {{ bottom_four }}" src="../../images/10.png"></image>
  </view>
</scroll-view>


</view> 


